	date-modified = {2023-11-12 18:51:31 -0800},
	doi = {10.1016/j.xinn.2022.100342},
	journal = {Innovation (Camb)},
	journal-full = {Innovation (Cambridge (Mass.))},
	month = {Nov},
	number = {6},
	pages = {100342},
	pmc = {PMC9637996},
	pmid = {36353677},
