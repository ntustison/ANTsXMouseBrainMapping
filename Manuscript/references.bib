	journal = {Neuroinformatics},
	journal-full = {Neuroinformatics},
	keywords = {MR-DTI; Parallel computing; Pipeline; Simulated atrophy; Validation methods; Voxel-based analysis},
	mesh = {Animals; Brain; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Mice; Multivariate Analysis; Neuroimaging; Reproducibility of Results},
	month = {Jul},
	number = {3},
