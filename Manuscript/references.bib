	doi = {10.1002/cne.24972},
	journal = {J Comp Neurol},
	journal-full = {The Journal of comparative neurology},
	keywords = {RRID:SCR-005988; RRID:SCR-007354; canids; dogs, white matter; domestication; evolution, gray matter; gyrification; scaling},
	mesh = {Animals; Animals, Wild; Biological Evolution; Brain Cortical Thickness; Brain Mapping; Canidae; Cerebral Cortex; Cognition; Dogs; Domestication; Gray Matter; Image Processing, Computer-Assisted; Magnetic Resonance Imaging; Species Specificity; White Matter},
	month = {Dec},
